.col_1
  %h2 Signup to Ideapi
  
  .boom_box

    = error_messages_for :user

    - form_for(:user, :url => users_path, :html => { :multipart => true }) do |f|
      = render :partial => "form", :locals => {:f => f}
      .hr
      
      =hidden_field_tag 'invite', @invite.code
      
      = f.submit "Sign up"