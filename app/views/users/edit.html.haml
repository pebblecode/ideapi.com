=title_holder do
  %h2 Edit profile

.col_1
  .boom_box
    %div[current_object]
      = error_messages_for :user

      - form_for(:user, :url => object_url, :html => { :method => :put, :multipart => true }) do |f|
        = render :partial => "form", :locals => {:f => f}
        %p= submit_tag "Update"
      %p.edit= link_to 'cancel', object_path
